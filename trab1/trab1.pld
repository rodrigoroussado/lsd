/* Start Here */
/*------------------------------------------------------*/
/* Inputs                                               */ 
/*------------------------------------------------------*/
PIN 1  = A;
PIN 2  = B;
PIN 3  = F1;
PIN 4  = F2;
PIN 5  = F3;
PIN 6  = FR;

/*------------------------------------------------------*/
/* Outputs                                              */
/*------------------------------------------------------*/
PIN 18 = OK;
PIN 14 = FSEL;

/*-------------------------------------------*/
/*LIGACOES*/
/*------------------------------------------ */
C0 = A;
C1 = B;

RR = FR;

FC0 = Y0;
FC1 = Y1;
E = FS;

RG = R;

OK = V;

FSEL = FS;


/*-------------------------------------------*/
/* CODIFICADOR COM PRIORIDADE                */
/* Inputs: F1, F2, F3                        */ 
/* Outputs: Y0, Y1, FS                       */
/*-------------------------------------------*/

Y0 = F3 # (F1&!F2&!F3);
Y1 = F2 # F3;
FS = F1 # F2 # F3;


/*-------------------------------------------*/
/* GERADOR DE FUNCAO                         */
/* Inputs: C0, C1, FC0, FC1                  */ 
/* Outputs: R                                */
/*-------------------------------------------*/

R = (C1&!FC0&FC1) # (!C0&C1&FC0) # (!C0&!FC0&FC1) # (!C1&FC0&!FC1) # (C0&!C1&FC0);


/*-------------------------------------------*/
/* VALIDADOR                                 */
/* Inputs: RG, E, RR                         */ 
/* Outputs: V                                */
/*-------------------------------------------*/

V = E&(RR&RG # !RR&!RG);
